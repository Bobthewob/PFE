@using STM.GDA.Web.Models
@model List<ComposantListeModel>

<h2>Liste des applications et services</h2>

@for (var i = 0; i < Model.Count; i++)
{
    <p>Nom</p>
    @Html.ValueFor(m => m[i].Nom); <br />
    <br />
    <p>Technologie</p>
    @Html.ValueFor(m => m[i].TechnologiesNom); <br />
    <br />

    <p>Dependance</p>
    @Html.ValueFor(m => m[i].DependancesNom); <br />
    <br />

}